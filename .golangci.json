{
  "formatters": {
    "enable": [
      "gofmt",
      "goimports"
    ],
    "exclusions": {
      "generated": "lax",
      "paths": [
        "third_party$",
        "builtin$",
        "examples$"
      ]
    },
    "settings": {
      "gofmt": {
        "simplify": false
      }
    }
  },
  "linters": {
    "default": "none",
    "enable": [
      "govet",
      "ineffassign",
      "misspell",
      "revive"
    ],
    "exclusions": {
      "generated": "lax",
      "paths": [
        "third_party$",
        "builtin$",
        "examples$"
      ],
      "presets": [
        "comments",
        "common-false-positives",
        "legacy",
        "std-error-handling"
      ],
      "rules": [
        {
          "linters": [
            "govet"
          ],
          "text": "^(nilness|structtag)"
        },
        {
          "linters": [
            "revive"
          ],
          "text": "should have comment"
        },
        {
          "linters": [
            "revive"
          ],
          "text": "should be of the form"
        },
        {
          "linters": [
            "revive"
          ],
          "text": "var-naming",
          "exclude": true
        }
      ]
    }
  },
  "run": {
    "tests": false
  },
  "version": "2"
}
